@model List<ResultAdviceViewModel>

<table class="table table-bordered" id="products_table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Ad Soyad</th>
            <th>Konu</th>
            <th>Email</th>
            <th>#</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AdviceId</td>
                <td>@item.FullName</td>
               
                <td>@item.Subject</td>
                <td>@item.Email</td>
                <td>
                    <a href="/Admin/Advice/Detail/@item.AdviceId" class="btn btn-info">Açıklamayı Gör</a>
                    <a href="/Admin/Advice/Delete/@item.AdviceId" class="btn btn-danger">Sil</a>
                </td>
            </tr>
        }
    </tbody>
</table>